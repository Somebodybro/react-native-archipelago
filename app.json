{
  "expo": {
    "name": "Archipela-Go!",
    "slug": "archipela-go",
    "version": "0.2.1",
    "orientation": "portrait",
    "icon": "./assets/archipela-go-logo.png",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-location",
        {
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled":true
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/archipela-go-logo.png",
        "backgroundColor": "#306D84"
      },
      "package": "com.aki665.archipelago",
      "permissions": ["ACCESS_BACKGROUND_LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6febcf2e-38be-40c3-9847-44eecc002f37"
      }
    }
  }
}
